{
  "name": "rels-analytics",
  "version": "2.0.0",
  "description": "Rels Analytics",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/reallyenglish/rels-analytics"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "underscore": "latest",
    "jquery": "latest"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.6.2",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "babel-plugin-handlebars-inline-precompile": "^2.1.1",
    "babelify": "^10",
    "brfs": "latest",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chalk": "latest",
    "colors": "*",
    "core-js": "^3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-chai-expect": "^2.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-mocha": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.0",
    "gulp": "4",
    "husky": "^3.0.0",
    "lolex": "latest",
    "natives": "^1.1.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rels-gulp": "git+ssh://git@github.com/reallyenglish/rels-gulp.git#v5.1.4",
    "semantic-release": "^15.13.25",
    "sinon": "^7",
    "sinon-chai": "^3"
  },
  "engine-strict": true,
  "engines": {
    "node": "^12",
    "npm": "^6"
  },
  "scripts": {
    "test": "gulp test --exit",
    "postinstall": "npm run build",
    "format": "prettier -c lib/**/*{.js*,.scss}",
    "lint": "eslint lib",
    "build": "./node_modules/.bin/babel ./lib --out-dir dist"
  },
  "release": {
    "getLastRelease": "./node_modules/last-release-git-tag/index.js",
    "debug": false
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "debug": false,
                "targets": {
                  "browsers": [
                    "Explorer 11",
                    "last 1 Chrome versions",
                    "last 1 FireFox versions",
                    "last 1 Safari versions",
                    "last 1 Edge versions"
                  ]
                },
                "useBuiltIns": "usage",
                "corejs": {
                  "version": "3.2",
                  "proposals": "true"
                }
              }
            ]
          ]
        }
      ]
    ]
  }
}
