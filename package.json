{
  "name": "rels-analytics",
  "version": "2.0.0",
  "description": "Rels Analytics",
  "main": "lib/rels-analytics.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/reallyenglish/rels-analytics"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "underscore": "latest",
    "jquery": "latest"
  },
  "devDependencies": {
    "require-dir": "latest",
    "gulp": "latest",
    "jshint-stylish": "latest",
    "mocha": "latest",
    "chai": "*",
    "chai-as-promised": "*",
    "sinon": "latest",
    "sinon-chai": "*",
    "rels-gulp": "git+ssh://git@github.com/reallyenglish/rels-gulp#v1.0.1",
    "ghooks": "^1.3.2",
    "last-release-git-tag": "latest",
    "validate-commit-msg": "^2.6.1",
    "semantic-release": "^4.3.5"
  },
  "engine-strict" : true,
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "test": "gulp test",
    "semantic-release": "semantic-release pre && semantic-release post"
  },
  "release": {
    "getLastRelease": "./node_modules/last-release-git-tag/index.js",
    "debug": false
  },
  "config": {
    "validate-commit-msg": {
      "helpMessage": "Valid types are: feat, fix, docs, style, refactor, perf, test, chore, revert \nNote that 'feat' and 'fix' will create new releases as will having BREAKING CHANGE: in the footer of your commit message."
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  }
}
